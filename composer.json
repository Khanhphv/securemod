{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.18",
        "ext-curl": "*",
        "ext-json": "*",
        "barryvdh/laravel-ide-helper": "^2.6",
        "fideloper/proxy": "^4.0",
        "jeroennoten/laravel-adminlte": "^3.5",
        "laravel/framework": "^7.30",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.0",
        "laravelcollective/html": "^6.2",
        "mews/captcha": "^3.2",
        "paypal/paypal-checkout-sdk": "1.0.1",
        "pragmarx/countries": "^0.7.2",
        "socialiteproviders/discord": "^2.0",
        "socialiteproviders/facebook": "^1.0",
        "stripe/stripe-php": "^7.75",
        "unisharp/laravel-filemanager": "^1.9",
        "webklex/laravel-imap": "^1.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^7.0",
        "kitloong/laravel-migrations-generator": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Library/NL_Checkoutv3.php",
            "app/Library/Helper.php",
            "app/Library/card_charging_api.php",
            "app/Library/cURL.php",
            "app/Library/CheckPayPalOrder.php",
            "app/Library/PaypalClient.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
